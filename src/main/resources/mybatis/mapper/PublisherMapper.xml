<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.sayone.mybatis.springbootmybatis.api.PublisherMapper">

    <resultMap type="com.sayone.mybatis.springbootmybatis.model.PublisherEntity" id="result">
        <id property="id" column="id" />
        <result property="firstName" column="first_name" />
        <result property="lastName" column="last_name" />
        <result property="email" column="email" />
        <result property="phoneNumber" column="phone_number" />
<!--        <result property="book" column="book" />-->
        <result property="role" column="role" />
    </resultMap>

    <select id="getAll" resultType="com.sayone.mybatis.springbootmybatis.model.PublisherEntity" resultMap="result">
        select * from publisher
    </select>

</mapper>