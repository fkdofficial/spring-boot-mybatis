<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.sayone.mybatis.springbootmybatis.api.UserMapper">

    <resultMap type="com.sayone.mybatis.springbootmybatis.model.BookEntity" id="result">
        <id property="bookId" column="book_id" />
        <result property="bookName" column="book_name" />
        <result property="author" column="author" />
        <result property="publisher" column="publisher" />
        <result property="genre" column="genre" />
        <result property="bookStatus" column="book_status" />
        <result property="yearOfPublication" column="year_of_publication" />
        <result property="pid" column="publisher_id" />
        <result property="bookDescription" column="book_description" />
    </resultMap>

    <select id="findAll" resultType="com.sayone.mybatis.springbootmybatis.model.BookEntity" resultMap="result">
        select * from book
    </select>

</mapper>